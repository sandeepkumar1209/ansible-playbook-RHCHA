var signupModalExpName = "growth_marketing_site_signup_modal_2022_11";

wf_experiments.initExperiments([
  {
    name: signupModalExpName,
    target: "logged_out",
    type: "custom",
    callback: function experimentVariantCallback(variant) {
      if (variant === "treatment") {
        wf_utils.signupModalUtils.selectModalVariant("treatment");
      } else {
        wf_utils.signupModalUtils.selectModalVariant("control");
      }
      wf_utils.onDocumentReady(function () {
        // Experiment will get assigned only if the signup modal is open:
        wf_utils.signupModalUtils.addModalOpenListener(function () {
          wf_experiments.assignExperiment(signupModalExpName);
        });
      });
    }
  }
]);
